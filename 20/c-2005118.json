ff0c\u805e\u8457\u8239\u5167\u5f15\u64ce\u767c\u51fa\u7684\u6750\u6cb9\u5473\uff0c\u660f\u660f\u6c89\u6c89\uff0c\u6688\u8457\u8239\uff0c\u56db\u7d1a\u6230\u5099\uff0c\u65b0\u5175\u54c0\u6028\u7684\u8aaa\uff0c\u6392\u526f\u6211\u5011\u771f\u7684\u8981\u6253\u6230\u55ce\uff1f\u6211\u8aaa\uff1a\u4f60\u64d4\u5fc3\u55ce\uff0c\u5225\u6015\u8ddf\u8457\u6211\uff0c\u770b\u770b\u4f60\u8eab\u908a\u7684\u4eba\uff0c\u4ed6\u5011\u90fd\u662f\u4f60\u751f\u5b58\u4e0b\u53bb\u7684\u7406\u7531\uff0c\u6015\u6b7b\u7684\u4eba\u6703\u5148\u6b7b\uff0c\u4ed6\u554f\uff1a\u6392\u526f\u4f60\u4e0d\u6015\u55ce\uff1f\u6211\u8aaa\uff1a\u6015\u600e\u9ebc\u4e0d\u6015\uff0c\u6211\u6015\u6c92\u6230\u722d\u53ef\u4ee5\u6253\uff0c\u53ea\u6709\u6230\u722d\u624d\u6703\u559a\u9192\u53f0\u7063\u4eba\uff0c\u624d\u6703\u8b93\u53f0\u7063\u4eba\u77e5\u9053\u73fe\u5728\u6709\u591a\u5e78\u798f\uff0c\u53ea\u6709\u6230\u722d\u751f\u5b58\u4e0b\u4f86\u7684\u4eba\uff0c\u624d\u80fd\u77e5\u9053\u4ee5\u524d\u7684\u524d\u8f29\u662f\u600e\u9ebc\u7528\u8840\uff0c\u7528\u8eab\u9ad4\u53bb\u634d\u885b\u6211\u5011\u73fe\u5728\u7684\u751f\u6d3b\uff0c\u7d50\u8ad6\u9084\u662f\u6c92\u6253\uff0c\u4e0d\u904e\u767c\u4e86\u56db\u679a\u98db\u5f48\u6e96\u78ba\u547d\u4e2d\u53f0\u7063\u96e2\u5cf6\u56db\u500b\u4f4d\u7f6e\uff0c\u4f86\u8d6b\u963b\u53f0\u7063\u7368\u7acb\u3002<br /><br />\u5728\u6d77\u8ecd\u9678\u6230\u968a\u7684\u751f\u6daf\u88e1\uff0c\u5b78\u4e86\u5f88\u591a\u7684\u6b66\u5668\uff0c\u62ff\u4e86\u5f88\u591a\u7684\u5e2b\u8cc7\uff0c\u52a9\u6559\u57f7\u7167\uff0c\u56e0\u6b64\u4e5f\u5e74\u6536\u5165\u6bd4\u4e00\u822c\u76f8\u540c\u968e\u5c64\u7684\u4eba\u9ad8\uff0c\u4f46\u6211\u5c07\u6211\u6240\u5b78\u7684\u6559\u80b2\u7d66\u6bcf\u500b\u58eb\u5175\uff0c\u6211\u56b4\u683c\u7684\u8981\u6c42\uff0c\u4f46\u4e0d\u904e\u5206\u7684\u64cd\u7df4\uff0c\u7528\u7684\u662f\u5982\u679c\u73fe\u5728\u6230\u722d\u4ed6\u5011\u80fd\u6d3b\u4e0b\u53bb\u7684\u7406\u7531\uff0c\u5c31\u662f\u6211\u7684\u8cac\u4efb\uff0c\u7528\u9019\u6a23\u7684\u60f3\u6cd5\u53bb\u64cd\u7df4\u6bcf\u500b\u58eb\u5175\uff0c\u6bcf\u500b\u5f1f\u5144\u90fd\u662f\u6230\u6642\u552f\u4e00\u7684\u4f9d\u9760\uff0c\u5c11\u4e86\u4e00\u500b\u5c31\u5982\u540c\u5931\u53bb\u5bb6\u4eba\uff0c\u653e\u5047\u9001\u5230\u5bb6\u9580\u53e3\u8ddf\u4ed6\u5011\u7236\u6bcd\u8acb\u5b89\uff0c\u6536\u5047\u5230\u5bb6\u88e1\u63a5\u4eba\uff0c\u4e00\u5b9a\u7b49\u5230\u4ed6\u5011\u624d\u4e00\u540c\u5750\u8eca\u6536\u5047\uff0c\u6211\u60f3\u61c9\u8a72\u5c11\u6578\u4eba\u624d\u8ddf\u6211\u4e00\u6a23\u5427\uff0c\u8ecd\u4eba\u4e0d\u8ad6\u98b3\u98a8\u4e0b\u96e8\uff0c\u53ea\u8981\u6c11\u9593\u6709\u4efb\u4f55\u554f\u984c\uff0c\u4e00\u5b9a\u90fd\u6703\u6d3e\u53bb\u7b2c\u4e00\u7dda\u5354\u52a9\uff0c\u4e0d\u7ba1\u5730\u9707\u98a8\u707d\u904e\u5f8c\uff0c\u90fd\u80fd\u5354\u52a9\u6c11\u9593\u91cd\u5efa\u5bb6\u5712\uff0c\u4fdd\u885b\u570b\u5bb6\u4e0d\u662f\u5b9a\u8981\u6709\u6230\u722d\uff0c\u5f37\u8eab\u5831\u570b\uff0c\u5c31\u5728\u6b64\u6642\uff0c\u8aaa\u5230\u4e5d\u4e8c\u4e00\uff0c\u6211\u540c\u5b78\u807d\u8aaa\u5f88\u591a\u4eba\u53bb\u5354\u52a9\uff0c\u8a31\u591a\u8ecd\u4eba\u5230\u4e86\u73fe\u5834\u770b\u5230\u7684\u6240\u505a\u7684\uff0c\u5728\u9000\u4f0d\u5f8c\u90fd\u6210\u4e86\u9670\u5f71\uff0c\u751a\u81f3\u6709\u4eba\u53d7\u4e0d\u4e86\u7576\u6642\u6240\u898b\u7684\u73fe\u6cc1\u81ea\u6bba\uff0c\u4e0d\u662f\u6bcf\u500b\u8ecd\u4eba\u90fd\u662f\u8caa\u7006\u5229\u5f0a\uff0c\u9084\u662f\u6709\u5f88\u591a\u8ecd\u4eba\u8eab\u9ad4\u529b\u884c\uff0c\u6211\u66fe\u7d93\u554f\u904e\u81ea\u5df1\uff0c\u70ba\u4ec0\u9ebc\u5225\u570b\u8ecd\u4eba\u5c0d\u81ea\u5df1\u570b\u5bb6\u7e3d\u662f\u5982\u6b64\u6df1\u4fe1\uff0c\u81ea\u4fe1\u6eff\u6eff\u8aaa\u51fa\u6211\u9858\u70ba\u570b\u72a7\u7272\uff0c\u53f0\u7063\u70ba\u4ec0\u9ebc\u6c92\u4eba\u8aaa\u6211\u9858\u70ba\u53f0\u7063\u72a7\u7272\uff0c\u56e0\u70ba\u53f0\u7063\u653f\u6cbb\u8150\u6557\uff0c\u53c8\u6df1\u5165\u8ecd\u4e2d\uff0c\u727d\u5f15\u8457\u9023\u8ecd\u968a\u9ad8\u5c64\u90fd\u88ab\u837c\u6bd2\u3002<br /><br />\u4f60\u8eab\u70ba\u8077\u696d\u8ecd\u4eba\uff0c\u5c31\u8a72\u505a\u597d\u4f60\u7684\u672c\u5206\uff0c\u4f60\u6709\u9019\u4e9b\u9592\u5de5\u592b\u5728\u9019\u6587\u7b46\u4e00\u756a\uff0c\u8868\u793a\u4f60\u4e5f\u662f\u904e\u5ea6\u5e7d\u9592\uff0c\u6211\u96d6\u7136\u7576\u4e09\u5e74\u534a\uff0c\u4f46\u6211\u6bcf\u5206\u6bcf\u79d2\u90fd\u5728\u60f3\u8457\u5f1f\u5144\uff0c\u6211\u80cc\u503c\u661f\u5c31\u80cc\u4e86\u5169\u5e74\u534a\uff0c\u653e\u5047\u4e00\u901a\u96fb\u8a71\u99ac\u4e0a\u56de\u8ecd\u4e2d\uff0c\u4f60\u9818\u5e74\u6536\u7d0480\u842c\uff0c\u6211\u9818\u5e74\u6536\u9084\u6bd4\u4f60\u591a\uff0c\u9019\u4e0d\u662f\u62ff\u51fa\u4f86\u7576\u8a71\u984c\u7684\u6771\u897f\uff0c\u4f60\u7684\u85aa\u8cc7\u662f\u570b\u5bb6\u4eba\u6c11\u7d0d\u7a05\u7d66\u4f60\uff0c\u5982\u679c\u4f60\u8a8d\u70ba\u56e0\u70ba\u4f60\u7684\u672c\u4e8b\u7406\u6240\u7576\u7136\u9818\u9019\u9ebc\u591a\uff0c\u90a3\u4f60\u5c31\u7406\u6240\u7576\u7136\u8a72\u628a\u4f60\u7684\u5de5\u4f5c\uff0c\u4f60\u7684\u672c\u8cea\u4f60\u7684\u5b78\u8853\u5949\u737b\u7d66\u4f60\u7684\u58eb\u5175\uff0c\u4f60\u8a8d\u70ba\u5f88\u5049\u5927\uff0c\u6230\u6642\u4f60\u6b7b\u4e86\u6c92\u4eba\u6709\u4f60\u7684\u6280\u80fd\uff0c\u56e0\u70ba\u4f60\u6c92\u7528\u5fc3\u6307\u5c0e\u4e0b\u5c6c\uff0c\u6c92\u4eba\u80fd\u50cf\u4f60\u4e00\u6a23\u62ef\u6551\u7576\u4e0b\u73fe\u6cc1\uff0c\u9020\u6210\u8239\u8266\u505c\u64fa\u6ec5\u4ea1\uff0c\u5c31\u662f\u4f60\u8a8d\u70ba\u7684\u5049\u5927\uff0c\u61c9\u8a72\uff0c\u5bb3\u6b7b\u6240\u6709\u7684\u5f1f\u5144\uff0c\u5982\u679c\u4f60\u80fd\u9019\u9ebc\u60f3\u662f\u5426\u6703\u66f4\u4e0a\u4e00\u5c64\u6a13\u3002<br /><br />\u7c73\u87f2\uff0c\u6bcf\u500b\u4eba\u90fd\u6703\u8aaa\u7c73\u87f2\uff0c\u4f60\u5011\u5403\u904e\u55ce\uff1f\u6211\u5403\u4e86\u4e09\u500b\u6708\u7684\u7c73\u87f2\uff0c\u8ecd\u968a\u8ecd\u7ce7\u5feb\u904e\u671f\uff0c\u5929\u5929\u62ff\u6230\u5099\u7c73\u7ce7\u51fa\u4f86\u5403\uff0c\u4e00\u7897\u98ef\u4e00\u534a\u4ee5\u4e0a\u542b\u8457\u7c73\u87f2\uff0c\u4f60\u4e0d\u5403\u55ce\uff1f\u5403\u600e\u9ebc\u4e0d